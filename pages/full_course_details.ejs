<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Details</title>
    <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="/vendor/boxicons/css/boxicons.css">
    <link rel="stylesheet" href="/vendor/bootstrap-icons/bootstrap-icons.css">
    <link rel="stylesheet" href="/css/jquery-ui.css">
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-light nav-style">
  <a class="navbar-brand" style="margin: right 0;" href="/home/"><img src="/images/kallyas-logo.svg" alt="TCFL LOGO" width="30%" height="30%" srcset=""></a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
         <div class="nav-content">
            <span class="bi-question-circle"></span>
          </div>
          <a class="nav-link item " href="/home/">Queries</a>
      </li>
      <li class="nav-item">
         <div class="nav-content">
            <span class="bi-vector-pen"></span>
          </div>
          <a class="nav-link item" href="/rec/notes/">Notes</a>
      </li>
         <li class="nav-item active">
         <div class="nav-content">
            <span class="bi-journal-bookmark" style="position: relative;bottom:10%"></span>
          </div>
          <a class="nav-link item active" href="/course_details/">Course Details</a>
      </li>
       <li class="nav-item">
         <div class="nav-content">
            <span class="bi-journal-bookmark" style="position: relative;bottom:10%"></span>
          </div>
          <a class="nav-link item" href="/dashboard/">Dashboard</a>
      </li>
    </ul>
  </div>
</nav>
<div class="page-content">
  <div class="container">
    <div id="tabs">
	    <ul>
		    <li><a href="#tabs-9">General Queries</a></li>
		    <li><a href="#tabs-10">S/Courses</a></li>
	    </ul>
    	<div id="tabs-9">
        <div class="table-responsive">
          <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">Course Id</th>
                <th scope="col">Name</th>
                <th scope="col">Duration</th>
                <th scope="col">Status</th>
                <th scope="col">Price</th>
            </tr>
            </thead>
            <tbody>
              <% if (data.length > 0) { %>     
                <% data.forEach(course => { %>
                  <% if (course.course_id >= 3000 && course.course_id <= 3999) { %>
                      <tr>
                        <th scope="row"><%= course.course_id%></th>
                        <td><%= course.course_name %></td>
                        <td><%= course.duration %></td>
                        <td>AVAILABLE</td>
                        <td><%= course.price%></td>
                      </tr>
                  <% } %>  
                <% }) %>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      <div id="tabs-10">
        <div class="table-responsive">
          <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">Course Id</th>
                <th scope="col">Name</th>
                <th scope="col">Duration</th>
                <th scope="col">Status</th>
                <th scope="col">Price</th>
            </tr>
            </thead>
            <tbody>
              <% if (data.length > 0) { %>     
                <% data.forEach(course => { %>
                  <% if (course.course_id >= 2000 && course.course_id <= 2999) { %>
                      <tr>
                        <th scope="row"><%= course.course_id%></th>
                        <td><%= course.course_name %></td>
                        <td><%= course.duration %></td>
                        <td>AVAILABLE</td>
                        <td><%= course.price%></td>
                      </tr>
                  <% } %>  
                <% }) %>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      </div>
    </div>
</div>
<script src="/vendor/jquery/jquery.js"></script>
<script src="/vendor/bootstrap/js/bootstrap.js"></script>
<script src="/js/jquery-ui.js"></script>
<script src="/js/main.js"></script>
</body>
</html>
